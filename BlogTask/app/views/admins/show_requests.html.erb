<% if @request_users %>
	<% @request_users.each do |request| %>
			<%= request. email %>
			<br>
			<%= "" %>
			<br>
			<!-- then add links to cancel and button to reject -->
			<%= link_to 'accept', admin_accept_request_path(user_request: request.id) %>
			|
			<%= link_to 'reject', admin_reject_request_path(user_request: request.id) %>
			<br>
		
	<% end %>
<% else %>
<%= "No requests to show" %>
<% end %>